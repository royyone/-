<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.example.springboot2.Dao.StudentMessageMapper">

    <!-- 通用查询映射结果 -->
    <resultMap id="BaseResultMap" type="com.example.springboot2.entity.Student">
        <id column="student_id" property="studentId" />
        <result column="student_username" property="studentUsername" />
        <result column="student_password" property="studentPassword" />
        <result column="truth_name" property="truthName" />
        <result column="student_college" property="studentCollege" />
    </resultMap>

    <!-- 通用查询结果列 -->
    <sql id="Base_Column_List">
        student_id, student_username, student_password, truth_name, student_college
    </sql>

</mapper>
